{
  "name": "ChatGPT-Archive",
  "version": "0.0.1",
  "description": "ChatGPT Archive is a Chrome extension designed to enhance the experience of users interacting with OpenAI's ChatGPT. This extension provides additional functionality that complements the standard capabilities of the AI model, focusing particularly on preserving, managing, and retrieving chat data.",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build --watch",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "dexie": "^3.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.238",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.14",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "fs-extra": "11.1.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "rollup": "3.25.3",
    "sass": "1.63.6",
    "tailwindcss": "^3.3.2",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "typescript": "5.1.3",
    "vite": "^4.3.9",
    "ws": "8.13.0"
  }
}
