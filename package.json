{
  "name": "ChatGPT-Archive",
  "version": "0.0.1",
  "description": "ChatGPT Archive enhances the ChatGPT experience by allowing you to manage conversations with ease.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build --watch",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts --configPlugin typescript",
    "build:prod": "tsc --noEmit && vite build && zip -r 'build.zip' dist/",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "dependencies": {
